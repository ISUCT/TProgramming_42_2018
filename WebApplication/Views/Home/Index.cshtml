@model IEnumerable<WebApplication.Models.Television>
@{
    ViewBag.Title = "Все смартфоны";
}
<a asp-action="Create" style="" class="button">Добавить модель телевизора</a>
<table class="table">
    <tr><td>Модель</td><td>Возраст</td><td>Цена</td><td></td></tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Name</td>
            <td>@item.Age</td>
            <td>@item.Price</td>
            <td>
            <a asp-action="Edit" asp-controller="Home" asp-route-id="@item.Id">Изменить</a> |
            <a asp-action="Delete" asp-controller="Home" asp-route-id="@item.Id">Удалить</a>
            </td>
        </tr>
    }
</table>

